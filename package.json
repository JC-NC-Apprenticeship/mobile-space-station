    "eslint-plugin-react-native": "^3.10.0",
    "expo-cli": "^4.0.15",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "jest-expo": "^41.0.0",
    "lint-staged": "^11.0.0",
    "markdownlint-cli": "^0.27.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
