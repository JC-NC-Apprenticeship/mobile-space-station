    "add": "^2.0.6",
    "base-64": "^1.0.0",
    "expo": "^41.0.1",
    "expo-barcode-scanner": "~10.1.2",
    "expo-crypto": "~9.1.0",
    "expo-local-authentication": "~11.0.2",
    "expo-notifications": "~0.11.6",
    "expo-permissions": "~12.0.1",
    "expo-secure-store": "~10.1.0",
